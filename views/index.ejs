<!DOCTYPE html>
<html lang="en">
   <head>
      <title>Tie</title>
      <% include partials/commonMeta %>
      <% include partials/commonCss %>
   </head>
   <body>
      <% include partials/navbar %>
      <!-- Icons Grid-->
      <section class="features-icons text-center">
         <div class="container">
            <div class="row">
               <% for(var i = 0; i < games.length; i++) {%>
                  <div class="col-lg-4">
                     <div class="features-icons-item mx-auto mb-5 mb-lg-0 mb-lg-3">
                        <img src="<%= games[i].thumbnail %>" height="200px" width="200px" />
                        <h3><a href="game/<%= games[i].title %>"><%= games[i].displayTitle %></a></h3>
                        <p class="lead mb-0"><%= games[i].description %></p>
                     </div>
                  </div>
               <% } %>
            </div>
         </div>
      </section>
      <% include partials/footer %>
      <% include partials/commonJs %>
   </body>
</html>

